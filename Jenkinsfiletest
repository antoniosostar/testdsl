
#!groovy

pipeline {
    agent any

    options {
        buildDiscarder(logRotator(numToKeepStr: '20', artifactDaysToKeepStr: '7', artifactNumToKeepStr: '5'))
        timeout(time: 1, unit: 'HOURS')
        timestamps()
    }

    stages {
        stage('Build XL Cli') {

            steps {
                sh "echo jej"
            }
        }
    }
    post {
        success {
            script {
                sh "echo da"
            }
        }
        failure {
            script {
                sh "echo ne"
            }
        }
    }
}
